{
  "name": "@bettermusic/studio",
  "version": "0.0.6",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/studio/studio.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "build.watch": "stencil build --watch --serve",
    "dev": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm -rf loader"
  },
  "dependencies": {
    "@bettermusic/chordsheetjs": "*",
    "@codemirror/commands": "^6.1",
    "@codemirror/lang-javascript": "^6.0",
    "@codemirror/lint": "^6.0",
    "@codemirror/state": "^6.1",
    "@codemirror/view": "^6.2",
    "@stencil/angular-output-target": "^0.4",
    "@stencil/core": "^2.18",
    "class-variance-authority": "^0.3.0",
    "codemirror-lang-chordpro": "*",
    "nanostores": "^0.7.1",
    "split.js": "^1.6.5"
  },
  "devDependencies": {
    "stencil-tailwind-plugin": "^1.6.0",
    "@bettermusic/eslint-config": "*",
    "@mertasan/tailwindcss-variables": "^2.5.1",
    "@rollup/plugin-typescript": "^9.0.2",
    "@stencil/postcss": "^2.1.0",
    "@stencil/sass": "^2.0",
    "@stencil/store": "^2.0",
    "@types/jest": "^27.4.5",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.23.1",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "postcss-import": "^15.0.0",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.0",
    "postcss-nested-ancestors": "^3.0.0",
    "postcss-simple-vars": "^7.0.1",
    "puppeteer": "^17.1.3",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tailwindcss": "^3.2.4"
  },
  "license": "MIT"
}
